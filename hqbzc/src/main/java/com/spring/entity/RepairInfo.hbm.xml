<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.spring.entity">
	<class name="RepairInfo" table="repairInfo">
		<id name="id" type="int">
			<column name="id" />
			<generator class="native" />
		</id>
		
		<property name="repair_name" type="string" column="repair_name" />
		<property name="content" type="string" column="content" />
		<property name="repair_type" type="string" column="repair_type" />
		<property name="time" type="string" column="time" />
		<property name="repair_address" type="string" column="repair_address" />
		
	</class>
</hibernate-mapping>