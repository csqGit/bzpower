<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.spring.entity">
	<class name="Admin" table="admin">
		<id name="id" type="int">
			<column name="id" />
			<generator class="native" />
		</id>
		
		<property name="admin_name" type="string" column="admin_name" />
		<property name="admin_id" type="string" column="admin_id" />
		<property name="admin_university" type="string" column="admin_university" />
		
	</class>
</hibernate-mapping>