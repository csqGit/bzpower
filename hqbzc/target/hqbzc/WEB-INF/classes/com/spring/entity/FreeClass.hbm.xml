<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.spring.entity">
	<class name="FreeClass" table="FreeClass">
		<id name="id" type="int">
			<column name="id" />
			<generator class="native" />
		</id>
		
		<property name="classId" type="int" column="classId" />
		<property name="className" type="string" column="className" />
		<property name="classAddress" type="string" column="classAddress" />
		<property name="classState" type="int" column="classState" />
		
	</class>
</hibernate-mapping>